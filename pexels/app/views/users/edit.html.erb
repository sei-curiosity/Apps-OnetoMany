<%= form_for @user, url: {action: "update"} do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <%= f.label :age %>
    <%= f.number_field :age%>
    <%= f.label :place %>
    <%= f.text_field :place%>
    <%= f.label :phone %>
    <%= f.text_field :phone%>
    <%= f.submit%>
<% end %>
